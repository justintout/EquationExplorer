<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eecs314.equationexplorer" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.eecs314.equationexplorer.ComputerArchitecture"
            android:label="Equation Explorer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.eecs314.equationexplorer.CPUTime"
            android:label="CPU Time"
            android:parentActivityName="com.eecs314.equationexplorer.ComputerArchitecture" >
            <meta-data
                android:name="andorid.support.PARENT_ACTIVITY"
                android:value="com.eecs314.equationexplorer.ComputerArchitecture"/>
        </activity>
    </application>
</manifest>
